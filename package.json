{
  "name": "my-repo-template-api",
  "author": "Kelscey Ortiz",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/src/index.js",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "tsx ./src",
    "ext": "ts, js"
  },
  "engines": {
    "node": "20"
  },
  "prisma": {
    "schema": "./src/database/schema.prisma"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "tsx dist/src",
    "build": "rimraf dist && tsc && tsc-alias && ncp public dist/public",
    "firebase:deploy": "yarn build && cross-env NODE_ENV=production firebase deploy --only functions,hosting",
    "firebase:serve": "firebase emulators:start --only functions",
    "db:generate": "prisma generate",
    "db:deploy-dev": "prisma migrate dev --name migration --create-only && prisma migrate deploy",
    "db:deploy-prod": "prisma generate && prisma migrate deploy",
    "db:reset": "prisma db push --force-reset && yarn db:deploy-dev",
    "db:seed": "tsx ./src/database/seeders/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.413.0",
    "@aws-sdk/s3-request-presigner": "^3.413.0",
    "@prisma/client": "^5.13.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "connect-pg-simple": "^9.0.0",
    "consola": "^3.2.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.6",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "ncp": "^2.0.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.4.0",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.10.2",
    "@types/pg-pool": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.13.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5",
    "cross-env": "^7.0.3"
  }
}
